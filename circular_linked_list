class Node:
    __slots__ ='element','next'

    def __init__(self):
        self.element = element
        self.next = next

class CirList:
    def __init__(self):
        self.head=None
        self.tail=None
        self.size=0

    def __len__(self):
        return self.size
    
    def isempty(self):
        return self.size==0

    def addlast(self,e):
        newest=Node(e,None)
        if self.isempty():
            newest.next=None
            self.head=newest
        else:
            newest.next=self.tail.next
            self.tail.next=newest
        self.tail=newest
        self.size+=1

    def display(self):
        p=self.head
        i=0
        while i< len(self):
            
            
        
